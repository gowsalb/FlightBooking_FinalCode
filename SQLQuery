
CREATE TABLE users(
	ID serial primary key,
	first_name varchar(150) not null,
    last_name varchar(150) not null,
	email varchar(200) unique not null,
	Password varchar(150) not null,
  Role VARCHAR(255)
);

CREATE TABLE Flights (
ID serial primary key,
  arrival_city VARCHAR(255),
  company_name VARCHAR(255),
  departure_city VARCHAR(255),
  flight_code integer(20),
  flight_name VARCHAR(255),
  No_Of_seatc_Available integer(30),
  Journey_Start_Time VARCHAR(255),
  Journey_End_Time VARCHAR(255),
  Cost_Per_Ticket Long
);

CREATE TABLE Flights (
ID serial primary key,
  first_name VARCHAR(255),
  last_name VARCHAR(255),
  email_ID VARCHAR(255),
  age integer(20),
  passport_number VARCHAR(255),
  CustomerID integer(30),
  BookingID VARCHAR(255),
  Booking_Status VARCHAR(255),
  flight_code integer(30),
  price_of_booking Long
);
